@model IEnumerable<TPIntegrador_PWeb3.DAL.DonacionesInsumos>

@{
    ViewBag.Title = "historial";
}

<h2>Historial de donaciones</h2><br />


<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Cantidad)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.NecesidadesDonacionesInsumos.Nombre)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Usuarios.Nombre)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Cantidad)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.NecesidadesDonacionesInsumos.Nombre)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Usuarios.Nombre)
        </td>
        
    </tr>
}

</table>
